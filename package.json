{
  "name": "Flashlight",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "clean-android": "cd android && ./gradlew clean",
    "build-bundle-android": "cd android && ./gradlew bundleRelease",
    "build-apk-android": "cd android && ./gradlew assembleRelease",
    "key-gen-android": "keytool -genkeypair -v -keystore ./android/app/release.keystore -alias androidrelease -keyalg RSA -keysize 2048 -validity 10000",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/hooks": "2.4.9",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-lantern": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@react-native-community/eslint-config": "1.0.0",
    "@types/jest": "24.0.24",
    "@types/react-native": "0.62.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "24.9.0",
    "eslint": "6.5.1",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.58.0",
    "prettier": "2.0.4",
    "react-native-rename": "2.4.1",
    "react-test-renderer": "16.11.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
