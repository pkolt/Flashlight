{
  "name": "Flashlight",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "clean-android": "cd android && ./gradlew clean",
    "build-bundle-android": "cd android && ./gradlew bundleRelease",
    "build-apk-android": "cd android && ./gradlew assembleRelease",
    "key-gen-android": "keytool -genkeypair -v -keystore ./android/app/release.keystore -alias androidrelease -keyalg RSA -keysize 2048 -validity 10000",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/hooks": "2.6.0",
    "react": "16.13.1",
    "react-native": "0.62.2",
    "react-native-i18n": "2.0.15",
    "react-native-lantern": "1.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/runtime": "7.10.2",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.0",
    "@types/react-native": "0.62.13",
    "@types/react-native-i18n": "2.0.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "babel-jest": "26.0.1",
    "eslint": "7.2.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "metro-react-native-babel-preset": "0.59.0",
    "npm-check-updates": "7.0.1",
    "prettier": "2.0.5",
    "react-native-rename": "2.4.1",
    "react-test-renderer": "16.13.1",
    "typescript": "3.9.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
